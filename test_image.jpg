    <script type="text/javascript">
        function loadGPTScript(callback) {
            // Avoid loading GPT twice
            if (document.querySelector('script[src*="gpt.js"]')) {
                callback?.(); // if (callback) { callback() }
                return;
            }

            const script = document.createElement('script');
            script.src = 'https://securepubads.g.doubleclick.net/tag/js/gpt.js';
            script.async = true;
            script.setAttribute('data-ot-ignore', '');
            script.onload = callback;
            document.head.appendChild(script);
        }

        function initGPT() {
            googletag.cmd.push(() => {
                window.gptManager.ads.forEach(ad => {
                    if (!googletag.pubads().getSlots().some(slot => slot.getSlotElementId() === ad.id)) {
                        ad._defineSlot(ad.isOutOfPage);
                    }
                });

                if (OneTrust?.IsAlertBoxClosedAndValid()) {
                    // Consent valid: display personalized ads
                    googletag.enableServices();
                    window.gptManager.ads.forEach(ad => {
                        googletag.display(ad.id);
                    });
                } else {
                    // Consent not valid: display non personalized ads ads
                    googletag.pubads().setRequestNonPersonalizedAds(1);
                    googletag.enableServices(); 
                }
            });
        }

        function OptanonWrapper() {
        // Case 1: User changes consent
            OneTrust.OnConsentChanged(function () {
                loadGPTScript(() => {
                    initGPT();
                });
            });

